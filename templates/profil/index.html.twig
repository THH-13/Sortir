{% extends 'base.html.twig' %}

{% block title %}User index{% endblock %}

{% block body %}
    <h1>User index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Email</th>
                <th>Pseudo</th>
                <th>Nom</th>
                <th>Prenom</th>
                <th>Telephone</th>
                <th>Campus_no_campus</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>

            <tr>
                <td>{{ app.user.id }}</td>
                <td>{{ app.user.email }}</td>
                <td>{{ app.user.pseudo }}</td>
                <td>{{ app.user.nom }}</td>
                <td>{{ app.user.prenom }}</td>
                <td>{{ app.user.telephone }}</td>
                <td>{{ app.user.campusNoCampus }}</td>
                <td>
                    <a href="{{ path('profil_edit', {'id': app.user.id}) }}">edit</a>
                </td>
            </tr>



        </tbody>
    </table>

{% endblock %}
